//Jenkins file only to tutorial example
pipeline {
  agent "ssh-agent-1"
  stages {       
    stage('Install dependencies') {
      steps {
        echo "Ingreso a instalar dependencias"
        nodejs(nodeJSInstallationName: 'node', configId: '<config-file-provider-id>') {
        sh 'npm config ls'
        sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         echo "Ingreso a realizar pruebas"
         sh 'npm test'
      }
    }      
  }
}
