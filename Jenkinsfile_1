//Jenkins file only to tutorial example
pipeline {
  agent "ssh-agent-1"
  tools {nodejs "node"}
  stages {       
    stage('Install dependencies') {
      steps {
        echo "Ingreso a instalar dependencias"
        sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         echo "Ingreso a realizar pruebas"
         sh 'npm test'
      }
    }      
  }
}
