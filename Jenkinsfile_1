//Jenkins file only to tutorial example
pipeline {
  agent any
  stages {       
    stage('Install dependencies') {
      steps {
        echo "Ingreso a instalar dependencias"
        nodejs(nodeJSInstallationName: 'node', configId: 'f64b5426-4f20-477f-9444-7f566d885a07') {
         sh 'npm config ls'
         sh 'npm install'
         }
       }
    }     
    stage('Test') {
      steps {
         echo "Ingreso a realizar pruebas"
         sh 'npm test'
      }
    }      
  }
}
